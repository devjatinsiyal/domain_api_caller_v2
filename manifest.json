{
    "name": "DOMAIN API calling extension",
    "version": "2.0",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_title": "Call the API on every tab"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "manifest_version": 3
}